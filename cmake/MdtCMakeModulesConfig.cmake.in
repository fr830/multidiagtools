@PACKAGE_INIT@

set(MDT_CMAKE_MODULE_PATH "${PACKAGE_PREFIX_DIR}/@MDT_CMAKE_MODULES_INSTALL_DIR@/modules")

# Add to CMAKE_MODULE_PATH if not allready
list(FIND CMAKE_MODULE_PATH "${MDT_CMAKE_MODULE_PATH}" MDT_CMAKE_MODULE_PATH_INDEX)
if(${MDT_CMAKE_MODULE_PATH_INDEX} LESS 0)
  set(CMAKE_MODULE_PATH "${MDT_CMAKE_MODULE_PATH}" ${CMAKE_MODULE_PATH})
endif()
